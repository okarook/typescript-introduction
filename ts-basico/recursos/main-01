const codigo: number = 1;
let nombre: string = 'Tobi';

console.log('log-1',`Codigo: ${codigo} - nombre: ${nombre}`);

const ciudades: string[] = ['Neiva', 'Bogota', 'Cali', 'Medellin'];
for( let row of ciudades) {
    console.log('log-ciudad', row);
}

const persona = {codigo: 1, nombre: 'Tobi', edad: 12, activo: true};
for( let row in persona) {
    console.log('log-persona', row);
}